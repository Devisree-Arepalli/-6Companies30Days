void rotate(vector<vector<int> >& a)
{
    // Your code goes here
    int i=0;
    vector<int>v;
    int n=a.size();
    while(i<n)
    {
        for(int j=0;j<n;j++)
        v.push_back(a[i][j]);
        
        i++;
    }
    int k=0,j=0;
    while(j<n)
    {
    for( i=n-1;i>=0;i--)
    {
        a[i][j]=v[k++];
    }
     j++;
}
}
